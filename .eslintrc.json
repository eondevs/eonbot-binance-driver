{
    "extends": "airbnb-base",
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-plusplus": 0,
        "indent": ["error", 4],
        "eqeqeq": 0,
        "no-mixed-operators": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "default-case": 0,
        "no-restricted-syntax": 0,
        "guard-for-in": 0,
        "max-len": 0,
        "no-param-reassign": 0
    }
}